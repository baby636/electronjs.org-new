{
    "root": "build/",
    "clean_urls": true,
    "headers":{
        "/**":{
            "Cache-Control": "no-cache"
        },
        "/assets/**":{
            "Cache-Control": "max-age=31536000, immutable"
        }
    },
    "routes": {
        "/docs/latest/assets/css/*": "/assets/css/",
        "/docs/latest/assets/js/*": "/assets/js/",
        "/docs/latest/assets/images/*": "/assets/images/"
    }
}